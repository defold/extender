{
  "family": "extender",
  "containerDefinitions": [
    {
      "image": "%IMAGE%",
      "name": "extender",
      "cpu": 10,
      "memory": 6144,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9000
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prod-logs",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "extender"
        }
      }
    }
  ]
}
